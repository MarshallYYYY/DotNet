<UserControl
    x:Class="Navigate.Views.BBBView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Navigate.Views"
    xmlns:viewmodels="clr-namespace:Navigate.ViewModels"
    d:DataContext="{d:DesignInstance Type=viewmodels:BBBViewModel}"
    mc:Ignorable="d"
    d:DesignHeight="450"
    d:DesignWidth="800">
    <Grid>
        <TextBlock
            FontSize="22"
            TextWrapping="Wrap"
            Text="{Binding Info}" />
    </Grid>
</UserControl>
