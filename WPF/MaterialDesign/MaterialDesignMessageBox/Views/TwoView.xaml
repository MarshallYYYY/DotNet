<Window
    x:Class="MaterialDesignMessageBox.Views.TwoView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:MaterialDesignMessageBox.Views"
    mc:Ignorable="d"
    Title="TwoView"
    Height="450"
    Width="800"
    xmlns:md="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:prism="http://prismlibrary.com/"
    prism:ViewModelLocator.AutoWireViewModel="True"
    xmlns:viewmodels="clr-namespace:MaterialDesignMessageBox.ViewModels"
    d:DataContext="{d:DesignInstance Type=viewmodels:TwoViewModel}">

    <Grid>

        <Button
            Margin="20"
            Content="MdMessageBox"
            Command="{Binding Path=OpenMdMessageBoxCommand}" />
    </Grid>
</Window>
